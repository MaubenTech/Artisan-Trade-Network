{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@components/*": ["src/components/*"],
			"@helpers/*": ["src/helpers/*"],
			"@constants/*": ["src/constants/*"],
			"@context/*": ["src/context/*"],
			"@assets/*": ["assets/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", "types/declaration.d.ts"]
}
